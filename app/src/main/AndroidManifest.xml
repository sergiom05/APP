<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.myfit">
<uses-permission android:name="android.permission.INTERNET"></uses-permission>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".RutinaSeisDías"></activity>
        <activity android:name=".RutinaCincoDías" />
        <activity android:name=".Recomendación"/>
        <activity android:name=".RutinaCuatroDías" />
        <activity android:name=".RutinaTresDías" />
        <activity android:name=".Rutinas" />
        <activity android:name=".CaloríasComida" />
        <activity android:name=".Temporizador" />
        <activity android:name=".PáginaPrincipal" />
        <activity android:name=".PerfilUsuario" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>